{
  "name": "generator-b-creative",
  "version": "0.1.0",
  "description": "Bornfight frontend project template based on gulp, es6 and scss",
  "homepage": "",
  "author": {
    "name": "Bornfight",
    "email": "frontend@bornfight.com",
    "url": "https://www.bornfight.com"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "frontend",
    "gulp",
    "es6",
    "babel",
    "scss",
    "browsersync",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.2",
    "figlet": "^1.3.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.1",
    "print-message": "^3.0.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.3.0"
  },
  "engines": {
    "npm": ">= 6.14.2"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "yeoman-generator": "^4.7.2",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "bornfight/b-creative",
  "license": "MIT"
}
